#include <stdio.h>

void main()
{
	int a, b;
	int result;

	a = 100;
	b = 50;

	result = a + b;
	print(" %d + %d = %d \n", a, b, result);

	result = a - b;
	print(" %d - %d = %d \n", a, b, result);

	result = a * b;
	print(" %d * %d = %d \n", a, b, result);

	result = a / b;
	print(" %d / %d = %d \n", a, b, result);

}

